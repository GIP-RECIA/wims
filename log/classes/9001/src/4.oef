\title{Disque (range)}
\language{fr}
\computeanswer{no}
\format{html}
\precision{10000}

\real{r = randint(100..500)/100}
\real{c = 2*pi*\r}
\real{valc = round(100*\c)/100}

\statement{Déterminer la circonférence  d'un disque de rayon \r.}

\answer{Circonférence}{\c - 0.01,\c + 0.01,\valc}{type=range}{option= default=0}
