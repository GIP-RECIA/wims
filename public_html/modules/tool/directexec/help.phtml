
!read themes/$wims_theme/table.proc
!if $special_parm=flydraw
  <h3 class="wimscenter">$name_doc $(swname[1])</h3>
  !set a=!record 0 of example.flydraw
  <div class="float_right">
  !insdraw $a
  </div>
  <textarea id="flydraw" cols="50" rows="15" name="wims_flydraw" readonly>
  !read example.flydraw
  </textarea>
  <br class="clearall">
  !read help/flydraw.phtml
  !exit
!endif

!if $special_parm=jsxgraph
  $name_remark_jsxgraph
  <textarea id="jsxbox" cols="100" rows="15" name="wims_jsxgraph" readonly>
  !read example.jsxscript
  </textarea>
  !goto common
!endif

!if $special_parm iswordof graphviz graphvizsvg
  !exec graphviz about

  <textarea id="jsxbox" cols="100" rows="15" name="wims_graphviz" readonly>
  !read example.graphviz
  </textarea>
  !goto common
!endif

!if $special_parm=canvasdraw
  <div>
  <textarea id="canvasdraw" cols="100" rows="15" name="wims_canvasdraw" readonly>
  !read example.canvasdraw
  </textarea>
  </div>
  !read help/canvasdraw.phtml
 !goto common
!endif
!if $(special_parm[1])=canvas_examples
  !set src=!record 0 of  help/canvas_examples/$(special_parm[2])
  $name_canvasdraw_example
  !read scripts/help/canvas_examples.phtml $(special_parm[2])
  !exec canvasdraw $src
  !read help/canvas_examples.phtml $(special_parm[2])
  !read help/canvasdraw.phtml
  !goto common
!endif

!if $special_parm=jmol
  $name_remark_jmol
  <textarea id="jmol" cols="100" rows="15" name="wims_jmol" readonly>
  !read example.jmol
  </textarea>
  !goto common
!endif
!if $special_parm=jsme
  $name_remark_jsme
  <textarea id="jsme" cols="100" rows="15" name="wims_jsme" readonly>
  !read example.jsme
  </textarea>
  !goto common
!endif
!if $special_parm=reactiondraw
  $name_remark_reactiondraw
  <textarea id="reactiondraw" cols="100" rows="15" name="wims_reactiondraw" readonly>
  !read example.reactiondraw
  </textarea>
  !goto common
!endif

!if $special_parm=pari
  !exec pari about
  <p>
   <a href="http://pari.math.u-bordeaux.fr/dochtml/html-stable/" target="wims_external">
   $name_doc $(swname[7])
   </a>
  </p>
  !goto common
!endif
:common

<form action="http://www.google.com/search">
<div>
<input type="submit" value="Google"> $name_doc
<select name="q">
!for sw in $swname
  <option value="$sw $name_doc">$sw</option>
!next sw
</select>
</div>
</form>

!if $special_parm!=flydraw
  <p>
  !href cmd=help&special_parm=flydraw $name_doc $(swname[1])
  .
  </p>
!endif
