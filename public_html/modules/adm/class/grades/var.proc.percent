method=list

per=$[$per]
!bound $per between 0 and 100 default 0

!for i=1 to $uucnt
  l_=!record $i+1 of wimshome/log/classes/$wims_class/.grades
  n_=!item 1 of $l_
  n_=!hex $n_
  g_$n_=$l_
!next i
update_content=!record 1 of wimshome/log/classes/$wims_class/.grades
update_content=!replace line number 1 by $per in $update_content
update_field=1
update_nbline=$empty
!read adm/uprecord wimshome/log/classes/$wims_class/.grades

!read var.init

