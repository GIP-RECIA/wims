
!read adm/css.phtml
!read tabletheme
!set wims_title_title=$(module_title_$lang)
!default wims_title_title=$module_title
!header
!read adm/class/classname.phtml $wims_classname\
$wims_institutionname

!read adm/title.phtml 1\
$classname

!if $error!=$empty
  <div class="wims_msg alert">
  !read lang/msg.phtml.$moduclass_lang
 </div>
  !goto end
!else
  !if $message!=$empty
    <div class="wims_msg success">
    $message
    </div>
  !endif
!endif

!read $job.phtml

:end
!!se trouve dans le menu du haut par le thème
!!set wims_menu_items=!append line class_home main,1,module=home\
to $wims_menu_items

!tail

!reset job dir iniparm
