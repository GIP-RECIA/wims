!set lang_exists=yes

!set name_passsup=password di amministrazione
!set name_password_check=Password security check
!set name_listclass=corso,modulo,coordinamento di corsi,corso,portale di sede

!default Cltype=$cltype
!if $wims_supertype = 2
  !default Cltype=$[$wims_supertype-1]
!endif
!if $Cltype iswordof 2 4
  !if $Cltype=2
    !set name_classe=$(name_listclass[$Cltype+1])
    !set name_sup=docente coordinatore
  !else
    !set name_classe=$(name_listclass[$Cltype+1])
    !set name_sup=docente supervisore
  !endif
  !set name_classes= il $name_classe
  !set name_classess= un $name_classe
  !set name_classesss= del $name_classe
  !set name_classessss= Il $name_classe
!else
  !set name_classe=!nosubst $(name_listclass[$Cltype+1])
  !set name_classes=!nosubst il $name_classe
  !set name_classess=!nosubst un $name_classe
  !set name_classesss=!nosubst del $name_classe
  !set name_classessss=!nosubst Il $name_classe
  !set name_sup=docente
!endif

# the name_cgu second item is the text link for the Terms of use.
!set name_cgu=Ho preso visione delle condizioni generali di utilizzo,\
  (leggi le CGU),\
  e le accetto.,\
