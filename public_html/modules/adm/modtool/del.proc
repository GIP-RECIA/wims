!if $confirm!=yes and / notin $modreq and .. notin $modreq
  test=!module title devel/$auth_login/$modreq
  !if $test!=$empty
    mod=$modreq
  !endif
  !exit
!endif

!if $mod=$empty
  !goto end
!endif

!if / isin $mod or .. isin $mod
  !goto end
!endif

!sh rm -fR $devdir/$mod

:end
job=modname
!changeto $job.proc
