$name_online
!set catcnt=!recordcnt lang/cat.$modu_lang
!set cat1cnt=!recordcnt lang/cat1.$modu_lang
!for i=1 to $cat1cnt
  !set l=!record $i of lang/cat1.$modu_lang
  !distribute lines $l into x1_$i,y1_$i
!next i
!for i=1 to $catcnt
  !set l=!record $i of lang/cat.$modu_lang
