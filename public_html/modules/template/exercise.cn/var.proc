# 变量计算应在此文件里进行.
# 每次模块调用时它由wims读入与解释执行.

!read language lang/names
!if $cmd=reply and $status!=waiting
  error=double_reply
  !exit
!endif

!if $cmd=reply and $status=waiting
  # 对回答的分析应该放在这里.
  # 以下几行只是一个模板.

  !if $x>$y
    diagnostics=good
  !else
    diagnostics=bad
  !endif

  status=done
  !if $diagnostics=good
    module_score=10
  !else
    module_score=0
  !endif
  wims_module_log=score $module_score/10
!endif
