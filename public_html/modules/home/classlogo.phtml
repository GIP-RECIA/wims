!set class_logo=!defof class_logo in wimshome/log/classes/$wims_class/.def

!if $class_logo issametext none or _ isin $wims_session
  !set class_logo=
  !exit
!endif
!if $class_logo issametext wims
  !set class_logo=gifs/logo.gif
!endif
!default class_logo=gifs/logo.gif
!bound class_logoside within left,right default left
