!set gl_n=!randint 3,6
!set gl_author=Bernadette, Perrin-Riou

!readproc slib/matrix/random $gl_n,$gl_n,1
!set gl_A1=!exec pari B=[$slib_out]; matrix($gl_n,$gl_n,i,j,if(i!=j,abs(B[i,j])))
!set gl_A2=!exec pari B=[$gl_A1]; B=B+mattranspose(B);matrix($gl_n,$gl_n,i,j,B[i,j]!=0)
!set gl_list=!makelist x for x=1 to $gl_n
!readproc slib/graph/draw [$gl_A1],blue,blue,[$gl_list],,html
!set gl_out1=$slib_out
!readproc slib/graph/draw [$gl_A2],blue,blue,[$gl_list],,html
!set gl_out2=$slib_out
