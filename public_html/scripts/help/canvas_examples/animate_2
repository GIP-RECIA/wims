size 400,400
xrange -10,10
yrange -10,10
axis
axisnumbering
precision 1
grid 2,2,grey,2,2,5,grey
precision 100
linewidth 4
fillcolor red
animate
trange -2*pi,2*pi
linewidth 1
opacity 255,50
canvastype 100
fill 1.2,1.2,red
canvastype 101
fill -1.2,-1.2,blue
multistrokecolors blue,blue,green,green,orange,orange
multilinewidth 2,2,3,3,1,1
jsplot blue,7*cos(x),5*sin(2*x),9*sin(x),5*cos(x),x^2,x
