slib_lang_exists_tmp=yes

slib_title=Simulation de réalisations d'une variable aléatoire suivant la loi binomiale.

slib_out=Simulation de <span class="tt">m</span> réalisations d'une variable aléatoire suivant la loi binomiale\
de paramètres <span class="tt">n</span> et <span class="tt">p</span> (<span class="tt">n</span> est un entier et <span class="tt">p</span> un réel entre 0 et 1).
slib_comment= P(X=k)=binomial(n,k)*p^k*(1-p)^(n-k) pour k=0,...,n.

