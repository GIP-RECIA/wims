slib_lang_exists_tmp=yes

slib_title=Arbre aléatoire
slib_parms=3\
,N nombre approximatif de noeuds\
,s degré maximal\
,h hauteur\
, texte\
, option : perfect (tous les noeuds ont même degré), middle, point

slib_out=[code pour dessiner un arbre à utiliser avec draw], \
[liste des noeuds et des enfants],[position des noeuds],\
[position des points ou des sommets]
slib_comment=option middle : dessin d'un cercle au milieu des arcs;\
option point : dessin d'un cercle sur les sommets
