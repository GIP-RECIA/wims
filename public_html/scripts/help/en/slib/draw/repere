slib_lang_exists_tmp=yes

slib_title=Draw a coordinate frame
slib_parms=11\
400,horizontal size of the drawing\
400,vertical size of the drawing\
20,margin around the frame\
-5,minimal abscissa\
5,maximal abscissa\
-5,minimal ordinate\
5,maximal ordinate\
1,step of graduation for abscissae\
1,step of graduation for ordinates\
black,axes colour\
grey,colour of the graduation grid

slib_out=several lines <ol>
<li>the coordinates of the origin of the frame in the drawing</li>\
<li>the scale for abscissae and ordinates</li>\
<li>third and following lines : data for insdraw</li></ol>
