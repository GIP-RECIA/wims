slib_lang_exists_tmp=yes

slib_title=Computes the coordinates of extreme points to draw a line in a frame
slib_parms=6\
,slope of the line\
,value at x=0\
,minimal abscissa of the frame\
,minimal ordinate of the frame\
,maximal abscissa of the frame\
,maximal ordinate of the frame

slib_out=Two lines, each  containing the coordinates of one of the points.