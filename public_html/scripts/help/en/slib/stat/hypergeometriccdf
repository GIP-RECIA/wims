slib_lang_exists_tmp=yes

slib_title=Cumulative distribution function of a hypergeometric law

slib_out=Values at <span class="tt">data</span> of cumulative distribution function of the hypergeometric law\
with parameters <span class="tt">N</span>, <span class="tt">K</span> and <span class="tt">n</span>.
slib_comment=<span class="tt">data</span> can be a list of reals.\
P(X=i)=binomial(K,i)*binomial(N-K,n-i)/binomial(N,n)

