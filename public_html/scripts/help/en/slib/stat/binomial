slib_lang_exists_tmp=yes

slib_title=Generation of binomial random data

slib_out=Random data of <span class="tt">m</span> integers following a binomial law of parameters <span class="tt">n</span> and <span class="tt">p</span> \
(<span class="tt">n</span> is an integer and <span class="tt">p</span> a real between 0 and 1).
slib_comment= P(X=k)=binomial(n,k)*p^k*(1-p)^(n-k) for k=0,...,n

