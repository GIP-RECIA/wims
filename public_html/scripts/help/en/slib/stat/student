slib_lang_exists_tmp=yes

slib_title=Generation of Student random data

slib_out=Random data of <span class="tt">m</span> reals following the Student\
 law with <span class="tt">n</span>degrees of freedom. \
If <span class="tt">m</span> is a list <span class="tt">[r,s]</span>\
, it returns an <span class="tt">r</span> by <span class="tt">s</span> matrix. 
