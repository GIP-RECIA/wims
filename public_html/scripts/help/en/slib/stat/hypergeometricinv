slib_lang_exists_tmp=yes

slib_title=Quantile of hypergeometric law inverse

slib_out=Quantile at <span class="tt">data</span> of the hypergeometric law inverse\
with parameters <span class="tt">N</span>, <span class="tt">K</span> and <span class="tt">n</span>.
slib_comment=<span class="tt">data</span> can be a list of reals;\
P(X=i)=binomial(K,i)*binomial(N-K,i)/binomial(N,n)

