!if $wims_read_parm!=slib_header
  !goto proc
!endif

slib_parms=3\
,[data]\
5,n\
0.5,p

slib_author=Sophie, Lemaire; Bernadette, Perrin-Riou

slib_example=[1,3,5],3,0.6
slib_require=octave
!exit

:proc

!distribute  item $wims_read_parm into slib_data,slib_n,slib_p

!default slib_n= 5
!default slib_p=0.5


!if $slib_p>=1 or $slib_p<=0
	slib_out=0
	!exit
!endif
slib_out=
slib_data=!declosing $slib_data

slib_cnt= !itemcnt $slib_data

!!deprecated syntax
!!slib_out=!exec octave pascal_cdf([$slib_data], $slib_n, $slib_p)
slib_out=!exec octave nbincdf([$slib_data] - $slib_n, $slib_n, $slib_p)

slib_out=!words2items $slib_out
slib_out=!trim $slib_out
