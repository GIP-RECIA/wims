!if $wims_read_parm!=slib_header
  !goto proc
!endif

slib_author=Georges, Khaznadar
slib_license=GPL

slib_example= H2\
C2H6\
HCO3^-\
Fe^3+ + 6CN^- -> Fe(CN)6^3-\
Fe^3+_aq + e^- -> Fe^2+_aq
slib_require=chemeq
!exit

:proc
!reset chemeq_option
chemeq_option=l
slib_out=!exec chemeq $wims_read_parm
slib_out=$slib_out
!reset chemeq_option
