!if $wims_read_parm!=slib_header
  !goto proc
!endif

slib_author=Georges, Khaznadar
slib_license=GPL

slib_example= Cr2O7^2- + 14H^+ + 6e^- -> 2 Cr^3+ + 7H2O\
Fe^3+ + e^- -> Fe^2+
slib_require=chemeq
!exit

:proc

!reset chemeq_option
chemeq_option=m
slib_out=!exec chemeq H -> H ~ $wims_read_parm
!reset chemeq_option


