!if $wims_read_parm!=slib_header
  !goto proc
!endif

slib_author=Georges, Khaznadar
slib_license=GPL

slib_example= Cu^2+ + 2e- -> Cu_s (0.34 V) ~ Fe^2+ + 2e- -> Fe_s (-1.44 V)\
Fe^2+ + 2e- -> Fe_s (-0.44 V)\
H2_g + 1/2 O2_g -> H2O\
N2_g + 3H2_g -> NH3_g (Kp=1e-6)
slib_require=chemeq
!exit

:proc

chemeq_option=w
slib_l=!exec chemeq $wims_read_parm
slib_out=!line 1 of $slib_l
!reset chemeq_option

